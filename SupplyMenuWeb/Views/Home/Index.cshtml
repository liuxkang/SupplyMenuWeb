@using SupplyMenuWeb.Models;
@model List<Supply>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Content/bootstrap.min.css"/>
    <script src="~/Scripts/jquery-1.9.1.js"></script>
    <script src="~/Scripts/MyScript.js"></script>
    <title>办公室办公用品库存</title>
    <style>
        img{
            width:inherit;
        }
        button.active
        {
            font:bold;
        }
    </style>
</head>
<body>
    <div class="container"> 
        <div class="col-lg-12">
            <img alt="办公室办公用品库存" src="~/Images/head.jpg" />
            <button class="btn active" id="all">全部</button>
            <button class="btn">文具</button>
            <button class="btn">日用品</button>
            <button class="btn">电脑配件</button>
            <button class="btn">打印机耗材</button>
            <button class="btn">其他耗材</button>
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>序号</th>
                        <th>货品名称</th>
                        <th>规格型号</th>
                        <th>单位</th>
                        <th>数量</th>
                        <th>备注</th>
                    </tr>
                </thead>
                <tbody>
                    @{ 
                        int i = 1;
                    }
                    @foreach (Supply su in Model)
                    {
                        <tr>
                            <td>@i</td>
                            <td>@su.SupplyName</td>
                            <td>@su.SupplyType</td>
                            <td>@su.Dw</td>
                            <td>@su.Count</td>
                            <td>@su.Remarks</td>
                        </tr>
                        i++;
                    }
                </tbody>
            </table>
        </div>
    </div>
</body>
</html>
